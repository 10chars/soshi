{
  "name": "prompter",
  "version": "0.1.2",
  "description": "a build tool",
  "main": "index.js",
  "scripts": {
    "flow": "flow || true",
    "start": "NODE_ENV=dev SERVER_LOCALE=localhost:9090 webpack-dev-server --content-base build --hot=true --port 9090 --host 0.0.0.0 --public localhost:9090 --config ./.builder/webpack.config.dev.js",
    "local": "webpack --config ./.builder/webpack.config.dev.js",
    "changelog": "SERVER_LOCALE=localhost node ./.builder/changelog.js",
    "versionList": "SERVER_LOCALE=localhost node ./.builder/versionMarker.js",
    "eslint": "eslint src --fix",
    "lint-staged": "lint-staged",
    "lint-pass": "echo '\\033[4;32m♡' Awsome! You are ready to commit ♡' \\033[0m'",
    "test": "TZ=Asia/Tokyo jest --verbose",
    "coverage": "npm run test -- --coverage",
    "build": "npm run test && node ./.builder/prompter.js",
    "prettify": "prettier --single-quote --print-width 120 --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'"
  },
  "jest": {
    "modulePaths": [
      "src",
      "components",
      "domain",
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/__mocks__/**",
      "!**/__tests__/**"
    ],
    "testRegex": "/__tests__/.*\\.(test|spec)\\.js$",
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "author": "Alexander King",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "bel": "^5.1.3",
    "child_process": "^1.0.2",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.11.2",
    "flow": "^0.2.3",
    "flow-babel-webpack-plugin": "^1.1.0",
    "flow-bin": "^0.53.1",
    "flow-typed": "^2.1.5",
    "git-commits": "^1.3.0",
    "google-closure-compiler-js": "^20170626.0.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^3.2.1",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "lint-staged": "^4.2.3",
    "nock": "^9.0.14",
    "node-sass": "^4.5.3",
    "ora": "^1.3.0",
    "postcss-font": "^1.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "prettier": "^1.6.1",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "universal-cookie": "^2.1.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --print-width 120 --trailing-comma es5 --write",
      "git add",
      "jest --findRelatedTests",
      "eslint ./src --cache --max-warnings 1"
    ]
  },
  "pre-commit": [
    "lint-staged",
    "lint-pass"
  ],
  "dependencies": {
    "base-element": "^3.0.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "recompose": "^0.25.0",
    "yo-yo": "^1.4.1",
    "yoyo": "^0.2.0"
  }
}
